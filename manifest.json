{
  "name": "Chrome Cache Nuke",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Easy button to clear your cache with your default options.",
  "browser_action": {
    "default_icon": "images/icon16.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/main.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "browsingData"
   ]
}
