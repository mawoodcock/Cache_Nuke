{
  "name": "Chrome Cache Nuke",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Easy button to clear your cache with your default options.",
  "browser_action": {
    "icons": {  "16": "images/icon16.png",
				"48": "images/icon48.png",
				"128": "images/icon128.png"
		     }	
  },
  "background": {
		"scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "browsingData",
	"http://*/*", 
    "https://*/*" 
   ]
}
